<body>

  <section id="register">

    <div class="col-sm-8">
      <div class="card">
        <div class="card-body">

          <form action="/configuration" method="POST">
            <div class="form-group">
              <label>Number of question each level</label>
              <input type="number" class="form-control" name="questionsPerLevel">
            </div>

            <button type="submit" class="btn btn-dark">Configure profile</button>
          </form>

        </div>
      </div>
    </div>

  </section>



